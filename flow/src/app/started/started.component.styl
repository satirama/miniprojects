.started
    margin-top 80px
    padding-bottom 50px
    .title
        background-color #F7F7F9
        padding-top 80px
        padding-bottom 30px
        color #2D343A
        h3 
            font-size 40px
        p  
            font-size 20px
            color #55595C
            font-weight 300
        .search
            position relative
            color #aaaaaa
            input  
                text-indent 32px
            .fa-search
                position absolute
                top 10px
                left 30px
    .get
        margin-top 40px
        .main
            p.edit 
                background-color #FCFBF5
            button 
                margin-right 20px
                i  
                    font-size 12px
        .side
            h4
                font-size 15px
                margin-bottom 15px
                font-weight 700
            p  
                line-height 0.6
                a  
                    color red
                a:hover
                    color #801212